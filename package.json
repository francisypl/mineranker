  "dependencies": {
    "aphrodite": "^1.1.0",
    "bluebird": "^3.4.7",
    "config": "^1.25.1",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "image-webpack-loader": "^3.2.0",
    "mobile-detect": "^1.3.5",
    "mongodb": "^2.2.24",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fa": "^4.1.2",
    "react-html5video": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-waypoint": "^5.1.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "restify": "^4.0.2",
    "superagent": "^3.5.0",
    "swagger-restify-mw": "^0.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.40.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "react",
      "stage-2",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7",
              "last 3 iOS versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/__mocks__/styleMock.js"
    }
  },
  "cacheDirectories": [
    "node_modules",
    "build"
  ],
  "scripts": {
    "start": "MINERANKER_API_URL=https://mineranker.herokuapp.com node app.js",
    "build": "npm run compile:app",
    "compile:app": "webpack -r dotenv/config --progress --profile --colors --display-error-details --display-cached",
    "compile:app:watch": "MINERANKER_API_URL=http://localhost:10010 webpack-dev-server -r dotenv/config --hot --inline --watch --progress --profile --colors --display-error-details --display-cached --content-base build/mineranker",
    "dev": "nodemon -w api --exec \"NODE_ENV=dev babel-node app.js --presets es2015,stage-0\"",
    "api:prod": "NODE_ENV=production babel-node app.js --presets es2015,stage-0",
    "edit": "swagger project edit",
    "flow": "flow check",
    "lint:app": "eslint app/",
    "postinstall": "npm run build",
    "test": "swagger project test",
    "test:app": "npm run flow && npm run lint:app && npm run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "engines": {
    "node": "6.4.0"
  }
}
